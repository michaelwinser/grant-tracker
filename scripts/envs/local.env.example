# Local development environment
# Copy to local.env and fill in your values
# Used by: ./gt start (Docker) or ./gt dev (Vite)

GCP_PROJECT_ID="your-project-id"
GCP_REGION="us-central1"

# OAuth credentials (from GCP Console -> APIs & Services -> Credentials)
GOOGLE_CLIENT_ID="your-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-client-secret"

# Local redirect URI - must be registered in OAuth client
REDIRECT_URI="http://localhost:8080/auth/callback"

# Not deployed to Cloud Run - runs locally
SERVICE_NAME="grant-tracker-local"
